{
  "$schema": "../node_modules/@tauri-apps/cli/schema.json",
  "build": {
    "beforeBuildCommand": "yarn build:tauri",
    "beforeDevCommand": "yarn start:tauri",
    "devPath": "http://localhost:3001",
    "distDir": "../build"
  },
  "package": {
    "productName": "cyb",
    "version": "0.1.6"
  },
  "tauri": {
    "allowlist": {
      "all": true
    },
    "bundle": {
      "active": true,
      "category": "DeveloperTool",
      "copyright": "",
      "deb": {
        "depends": []
      },
      "externalBin": [],
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "ai.cyb.app",
      "longDescription": "",
      "macOS": {
        "entitlements": null,
        "exceptionDomain": "",
        "frameworks": [],
        "providerShortName": null,
        "signingIdentity": null
      },
      "resources": [],
      "shortDescription": "",
      "targets": "all",
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": false
    },
    "windows": [
      {
        "label": "main",
        "fullscreen": false,
        "height": 760,
        "resizable": true,
        "title": "BOSTROM cyb.ai",
        "width": 1200
      },
      {
        "label": "splashscreen",
        "fullscreen": false,
        "height": 760,
        "resizable": false,
        "decorations": false,
        "title": "BOSTROM cyb.ai",
        "width": 1200,
        "url": "splashscreen.html"
      }
    ]
  }
}
